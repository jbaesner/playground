<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE jboss-app PUBLIC "-//JBoss//DTD J2EE Application 4.2//EN" "http://www.jboss.org/j2ee/dtd/jboss-app_4_2.dtd">
<jboss-app>
	<security-role>
		<!-- Each principal having the role 'app' will have this role replaced with 'app' and 'cus' -->
		<role-name>app</role-name>
		<principal-name>app</principal-name>
		<principal-name>cus</principal-name>
	</security-role>
</jboss-app>